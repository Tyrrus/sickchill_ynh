{
    "name": "SickChill",
    "id": "sickchill",
    "packaging_format": 1,
    "description": {
        "en": "Automatic TV shows downloader",
        "fr": "Téléchargement automatisé de séries TV"
    },
    "version": "1.0~ynh1",
    "url": "https://sickchill.github.io/",
    "license": "free",
    "maintainer": {
        "name": "Tyrrus",
        "url": "https://github.com/Tyrrus"
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for SickChill",
                    "fr": "Choisissez un nom de domaine pour SickChill"
                },
                "example": "mydomain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for ynhexample",
                    "fr": "Choisissez un chemin pour ynhexample"
                },
                "example": "/sickchill",
                "default": "/sickchill"
            }
        ]
    }
}
